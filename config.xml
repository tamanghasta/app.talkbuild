<?xml version='1.0' encoding='utf-8'?>
<widget id="talk.build" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TalkBuild</name>
    <description>
       Social Media Network for construction professionals
    </description>
    <author email="developers@cfadigital.com" href="http://cfa-marketing.com">
        Cfa Developers
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="icon.png" />

    <!-- Define app icon for each platform. -->
    <icon src="icon.png" />

    <icon src="res/icon/android/icon-48-mdpi.png"   gap:platform="android"    gap:qualifier="mdpi" />
    <icon src="res/icon/android/icon-72-hdpi.png"   gap:platform="android"    gap:qualifier="hdpi" />
    <icon src="res/icon/android/icon-96-xhdpi.png"  gap:platform="android"    gap:qualifier="xhdpi" />
</widget>
